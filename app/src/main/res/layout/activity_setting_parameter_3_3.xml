<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SettingParameter_3_3">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Cài đặt thông số cho luống cây"

        android:textSize="25sp"
        android:textStyle="bold"
        android:drawablePadding="20dp"
        android:fontFamily="@font/noto_sans_cjk_jp_bold"
        android:gravity="center_vertical"
        android:padding="10dp"
        android:textColor="#000000"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText,RtlHardcoded,UnusedAttribute" />


    <ScrollView
        android:id="@+id/scrollView3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:layout_constraintVertical_bias="1.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <Button
                android:id="@+id/btn_AcceptSetting"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                android:background="@drawable/btn_login"
                android:padding="10dp"
                android:textStyle="bold"
                android:text="Xác nhận cài đặt"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView15"
                tools:ignore="HardcodedText" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:drawablePadding="20dp"
                android:fontFamily="@font/noto_sans_cjk_jp_bold"
                android:gravity="center_vertical"
                android:padding="10dp"
                android:text="Độ dinh dưỡng của đất"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toStartOf="@+id/edt_Humidity_Max_Land"
                app:layout_constraintStart_toStartOf="@+id/textView13"
                app:layout_constraintTop_toBottomOf="@+id/textView13"
                tools:ignore="HardcodedText,UnusedAttribute" />


            <TextView
                android:id="@+id/textView10"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginTop="16dp"
                android:textStyle="bold"
                android:drawablePadding="20dp"
                android:fontFamily="@font/noto_sans_cjk_jp_bold"
                android:gravity="center_vertical"
                android:padding="10dp"
                android:text="Ngưỡng trên độ ẩm đất"
                android:textColor="#000000"

                android:textSize="20sp"
                app:layout_constraintEnd_toStartOf="@+id/edt_Humidity_Max_Land"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="HardcodedText,RtlHardcoded,UnusedAttribute" />

            <EditText
                android:id="@+id/edt_Humidity_Max_Land"
                android:layout_width="64sp"
                android:layout_height="wrap_content"
                android:drawablePadding="20dp"
                android:ems="10"
                android:fontFamily="@font/noto_sans_cjk_jp_bold"
                android:gravity="center"
                android:inputType="number"
                android:padding="10dp"

                android:textColor="#FFFFFF"
                android:textSize="20sp"

                app:layout_constraintEnd_toStartOf="@+id/textView11"
                app:layout_constraintTop_toTopOf="@+id/textView10"
                tools:ignore="Autofill,HardcodedText,LabelFor,UnusedAttribute" />

            <EditText
                android:id="@+id/edt_Humidity_Air"
                android:layout_width="64sp"
                android:layout_height="wrap_content"
                android:drawablePadding="20dp"
                android:ems="10"
                android:fontFamily="@font/noto_sans_cjk_jp_bold"
                android:gravity="center"
                android:inputType="number"
                android:padding="10dp"
                android:textColor="#000000"
                android:textSize="20sp"
                app:layout_constraintEnd_toStartOf="@+id/textView11"
                app:layout_constraintTop_toTopOf="@+id/textView112"
                tools:ignore="Autofill,HardcodedText,LabelFor,UnusedAttribute" />

            <EditText
                android:id="@+id/edt_Temperature_Evi"
                android:layout_width="64sp"
                android:layout_height="wrap_content"
                android:drawablePadding="20dp"
                android:ems="10"
                android:fontFamily="@font/noto_sans_cjk_jp_bold"
                android:gravity="center"
                android:inputType="number"
                android:padding="10dp"
                android:textColor="#000000"
                android:textSize="20sp"
                app:layout_constraintEnd_toStartOf="@+id/textView11"
                app:layout_constraintTop_toTopOf="@+id/textView13"
                tools:ignore="Autofill,HardcodedText,LabelFor,UnusedAttribute" />

            <EditText
                android:id="@+id/edt_Nutri"
                android:layout_width="64sp"
                android:layout_height="wrap_content"
                android:drawablePadding="20dp"
                android:ems="10"
                android:fontFamily="@font/noto_sans_cjk_jp_bold"
                android:gravity="center"
                android:inputType="number"
                android:padding="10dp"
                android:textColor="#000000"
                android:textSize="20sp"
                app:layout_constraintEnd_toStartOf="@+id/textView11"
                app:layout_constraintTop_toTopOf="@+id/textView15"
                tools:ignore="Autofill,HardcodedText,LabelFor,UnusedAttribute" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:layout_marginRight="16dp"
                android:drawablePadding="20dp"
                android:fontFamily="@font/noto_sans_cjk_jp_bold"
                android:gravity="center_vertical"
                android:padding="10dp"
                android:text="%"
                android:textColor="#000000"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/edt_Humidity_Max_Land"
                tools:ignore="HardcodedText,RtlHardcoded,UnusedAttribute" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawablePadding="20dp"
                android:fontFamily="@font/noto_sans_cjk_jp_bold"
                android:gravity="center_vertical"
                android:padding="10dp"
                android:text="%"
                android:textColor="#000000"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="@+id/textView11"
                app:layout_constraintTop_toTopOf="@+id/edt_Humidity_Air"
                tools:ignore="HardcodedText,UnusedAttribute" />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawablePadding="20dp"
                android:fontFamily="@font/noto_sans_cjk_jp_bold"
                android:gravity="center_vertical"
                android:padding="10dp"
                android:text="%"
                android:textColor="#000000"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="@+id/textView14"
                app:layout_constraintTop_toTopOf="@+id/edt_Nutri"
                tools:ignore="HardcodedText,UnusedAttribute" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawablePadding="20dp"
                android:fontFamily="@font/noto_sans_cjk_jp_bold"
                android:gravity="center_vertical"
                android:padding="10dp"
                android:text="°C"
                android:textColor="#000000"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="@+id/textView12"
                app:layout_constraintTop_toTopOf="@+id/edt_Temperature_Evi"
                tools:ignore="HardcodedText,UnusedAttribute" />

            <TextView
                android:id="@+id/textView112"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:drawablePadding="20dp"
                android:fontFamily="@font/noto_sans_cjk_jp_bold"
                android:gravity="center_vertical"
                android:padding="10dp"
                android:textStyle="bold"
                android:text="Độ ẩm không khí"
                android:textColor="#000000"
                android:textSize="20sp"

                app:layout_constraintEnd_toStartOf="@+id/edt_Humidity_Max_Land"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="@+id/textView10"
                app:layout_constraintTop_toBottomOf="@+id/textView19"
                tools:ignore="HardcodedText,UnusedAttribute" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:drawablePadding="20dp"
                android:fontFamily="@font/noto_sans_cjk_jp_bold"
                android:gravity="center_vertical"
                android:padding="10dp"
                android:text="Nhiệt độ môi trường"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toStartOf="@+id/edt_Humidity_Max_Land"
                app:layout_constraintStart_toStartOf="@+id/textView112"
                app:layout_constraintTop_toBottomOf="@+id/textView112"
                tools:ignore="HardcodedText,UnusedAttribute" />

            <TextView
                android:id="@+id/textView19"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"

                android:drawablePadding="20dp"
                android:fontFamily="@font/noto_sans_cjk_jp_bold"
                android:gravity="center_vertical"
                android:padding="10dp"
                android:textStyle="bold"
                android:text="Ngưỡng trên độ ẩm đất"
                android:textColor="#000000"

                android:textSize="20sp"

                app:layout_constraintEnd_toStartOf="@+id/edt_Humidity_Max_Land"
                app:layout_constraintStart_toStartOf="@+id/textView10"
                app:layout_constraintTop_toBottomOf="@+id/textView10"
                tools:ignore="HardcodedText" />

            <EditText
                android:id="@+id/edt_Humidity_Min_Land"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:fontFamily="@font/noto_sans_cjk_jp_bold"
                android:gravity="center"
                android:inputType="number"
                android:padding="10dp"
                android:textSize="20dp"
                app:layout_constraintBottom_toBottomOf="@+id/textView19"
                app:layout_constraintEnd_toStartOf="@+id/textView11"
                app:layout_constraintStart_toStartOf="@+id/edt_Humidity_Max_Land"
                app:layout_constraintTop_toTopOf="@+id/textView19"
                tools:ignore="Autofill,HardcodedText,LabelFor,SpUsage" />

            <TextView
                android:id="@+id/textView20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawablePadding="20dp"
                android:fontFamily="@font/noto_sans_cjk_jp_bold"
                android:gravity="center_vertical"
                android:padding="10dp"
                android:text="%"
                android:textColor="#000000"
                android:textSize="20sp"

                app:layout_constraintBottom_toBottomOf="@+id/edt_Humidity_Min_Land"
                app:layout_constraintEnd_toEndOf="@+id/textView11"
                app:layout_constraintStart_toStartOf="@+id/textView11"
                app:layout_constraintTop_toTopOf="@+id/edt_Humidity_Min_Land"
                tools:ignore="HardcodedText,SpUsage" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>